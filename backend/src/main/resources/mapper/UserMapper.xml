<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="xcmg.global.cockpit.backend.mapper.UserMapper">

    <insert id="insertBaseInfo" parameterType="xcmg.global.cockpit.backend.vo.param.UserParamVO">
        insert into user(username, phone, age)
        values (#{param.username},
                #{param.phone},
                #{param.age})
    </insert>

    <select id="getBaseInfo" resultType="xcmg.global.cockpit.backend.vo.param.UserParamVO">
        select
          username,
          phone,
          age
        from user
    </select>
</mapper>
